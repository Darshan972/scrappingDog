{
  "name": "n8n-nodes-scraping-dog",
  "version": "0.3.4",
  "description": "A custom n8n node for integrating with ScrapingDog to perform web scraping tasks.",
  "keywords": [
    "n8n",
    "n8n-node",
    "scrapingdog",
    "web-scraping",
    "automation",
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Darshan Khandelwal",
    "email": "darshankhandelwal12@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Darshan972/scrappingDog.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/scrapingDogApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ScrapingDog/ScrapingDog.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/luxon": "^3.4.0",
    "@types/node": "^24.0.7",
    "gulp": "^5.0.1",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.0",
    "typescript": "^5.8.2",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {}
}
